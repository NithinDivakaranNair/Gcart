        <!--header-->
     
        <main class="container light-style flex-grow-1 container-p-y">
        <section class="h-10 gradient-custom">
            <div class="container py-5 h-100">
              <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col-lg-10 col-xl-8">
                  <div class="card" style="border-radius: 10px;">
                    <div class="card-header px-4 py-5">
                      <h4 class="text-dark mb-0">Order Details</h4>
                      <p class="lead fw-normal mb-0" style="color: #12020e;">Oder Id:<%=selectedorderdetails._id%></p>
                    </div>
                    <div class="card-body p-4">
                      <div class="d-flex justify-content-between align-items-center mb-4">
                        
                        <!-- <p class="small text-muted mb-0">Receipt Voucher : 1KAU9-84UIL</p> -->
                      </div>
                      <div class="d-flex justify-content-between align-items-center mb-4">
                        <p class="lead fw-normal mb-0" style="color: #090709;">
                          Name: <%= selectedorderdetails.address.Name %><br>
                          Address: <%= selectedorderdetails.address.Address %>
                        </p>
                        
                        <!-- <p class="small text-muted mb-0">Receipt Voucher : 1KAU9-84UIL</p> -->
                      </div>
                      
                   
                      <div class="card shadow mb-4">
                        <div class="card-body">
                          <% selectedorderdetails.iteams.forEach((item) => { %>
                          <div class="row">
                            <div class="col-md-2 mb-2">
                              <img src="/uploads/<%= item.Image[0] %>" style="height: 60px; width: 60px;" alt="Phone">
                            </div>
                            <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0"><%= item.Model %></p>
                            </div>
                            <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0 small"><%= item.Description %></p>
                            </div>
                          
                            <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                              <% if (item.OfferPrice) { %>
                              <p class="text-dark mb-0 small"><%= item.OfferPrice.toLocaleString('en-IN', { style: 'currency', currency: 'INR' }) %></p>
                              <% } else { %>
                              <p class="text-dark mb-0 small"><%= item.Price.toLocaleString('en-IN', { style: 'currency', currency: 'INR' }) %></p>
                              <% } %>
                            </div>
                            <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0 small"><%= item.Count %></p>
                            </div>
                            <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                              <% if (item.OfferPrice) { %>
                                  <p class="text-dark mb-0 small"><%= (item.OfferPrice * item.Count).toLocaleString('en-IN', { style: 'currency', currency: 'INR' }) %></p>
                              <% } else { %>
                                  <p class="text-dark mb-0 small"><%= (item.Price * item.Count).toLocaleString('en-IN', { style: 'currency', currency: 'INR' }) %></p>
                              <% } %>
                          </div>
                          
                            
                            <div class="col-md-2 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0 small"><%= item.Brand %></p>
                            </div>
                          </div>
                          <% }) %>
                      


                          <hr class="mb-4" style="background-color: #e0e0e0; opacity: 1;">
                         
                          <div class="row">
                            <div class="col-md-6 text-center d-flex justify-content-center align-items-center">
                              <p class="fw-bold mb-0">Discount</p>
                            </div>
                            <div class="col-md-6 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0 small">20%</p>
                            </div>
                          </div>
                          <div class="row">
                            <div class="col-md-6 text-center d-flex justify-content-center align-items-center">
                              <p class="fw-bold mb-0">Delivery Charge</p>
                            </div>
                            <div class="col-md-6 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0 small"><%=(50).toLocaleString('en-IN', { style: 'currency', currency: 'INR' }) %>+</p>
                            </div>
                          </div>
                          
                        <% if(selectedorderdetails.Coupon){ %>
                          <hr class="mb-4" style="background-color: #e0e0e0; opacity: 1;">
                          <div class="row">
                            <div class="col-md-6 text-center d-flex justify-content-center align-items-center">
                              <p class="fw-bold mb-0">Applied Coupon</p>
                            </div>
                            <div class="col-md-6 text-center d-flex justify-content-center align-items-center">
                              <p class="text-dark mb-0 small"><%= selectedorderdetails.Coupon %>-</p>
                            </div>
                          </div>
                          <% } %>
                          <hr class="mb-4" style="background-color: #e0e0e0; opacity: 1;">
                      
                          <div class="row d-flex align-items-center">
                            <div class="col-md-2 fw-bold mb-0">
                              <p class="fw-bold mb-0">Order Status:</p>
                            </div>
                            <div class="col-md-10">
                              <div class="d-flex justify-content-around mb-1">
                                <% if (selectedorderdetails.orderstatus == "ordercancelled") { %>
                                <p class="text-dark mt-1 mb-0 small ms-xl-5">Order Canceled</p>
                               <%console.log("selectedorderdetails.orderstatus:",selectedorderdetails.orderstatus)  %>
                                <% } else if(selectedorderdetails.orderstatus=="Order is returned") { %>
                                  <p class="text-dark mt-1 mb-0 small ms-xl-5">Order is returned</p>
                                  <% }else{ %>
                                <p class="text-dark mt-1 mb-0 small ms-xl-5"><%= selectedorderdetails.orderstatus %></p>
                                <% } %>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>
                      
             
