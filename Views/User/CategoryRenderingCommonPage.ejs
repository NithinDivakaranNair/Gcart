    <!-- sidebar + content -->
    <section>
      <div class="container my-5">
        <div class="row">
          <% prodectinfo.forEach(data => { %>
            
          <div class="col-lg-3 col-md-6 col-sm-6 d-flex">
            <a href="/oneprodectdetails/<%= data._id %>">
              <div class="card w-100 my-2 shadow-2-strong">
                <img src="/uploads/<%= data.Image[0] %>" alt="Product Image" width="200" height="210" />
                <div class="card-body d-flex flex-column">
                  <h6><a href="#"><%= data.Brand %></a></h6>
                  <h6><a href="#"><%= data.Model %></a></h6>
                  <h6><a href="#"><%= data.Description %></a></h6>
                  <!-- Uncomment the rating section when needed -->
                  <!-- <div class="rating">
                    <i class="fa fa-star"></i>
                    <i class="fa fa-star"></i>
                    <i class="fa fa-star"></i>
                    <i class="fa fa-star"></i>
                    <i class="fa fa-star"></i>
                  </div> -->
                  <div class="product__price"><%= data.Price %></div>
                  <div class="card-footer d-flex align-items-end pt-3 px-0 pb-0 mt-auto">
                      <a href="#!" class="btn btn-success shadow-0 me-1">Add to cart</a>
                      <a href="#!" class="btn btn-warning shadow-0 me-1">Wishlist</a>
                    </div>
                    
                </div>
              </div>
            </a>
          </div>
          <% }) %>
        </div>
      </div>
