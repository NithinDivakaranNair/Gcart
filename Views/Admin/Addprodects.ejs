            <div class="container mt-4">
              <div class="my-5"> <!-- Add margin top -->
                  <h1>Add Products</h1>
              </div>
            <form action="/addprodects"  method="post"  enctype="multipart/form-data" ><!--multer funtionality working (enctype="multipart/form-data") this code is adding for post methode-->
               
                 <div class="mb-3">
                 
                  <label for="Category" class="form-label">Category</label>
                 <select class="form-select" id="Category" name="Category">
                  <option selected>Select a Category</option>
              <% Cdetails.forEach((element) => { %>
                      <option value="<%= element.Category %>" style="color: rgb(28, 4, 4); background-color: yellow;">
                        <%= element.Category %>
                      </option>
                  <% }); %>
                </select>
                </div>
            
               <div class="mb-3">
               <label for="Brand" class="form-label text-white">Brand</label>
               <textarea class="form-control text-white" id="Brand" name="Brand"></textarea>
               </div>
        
                <div class="mb-3">
                    <label for="Model" class="form-label text-white">Model</label>
                    <textarea class="form-control text-white" id="Model" name="Model"></textarea>
                </div>
                <div class="mb-3">
                  <label for="Image" class="form-label text-white">Image</label>
                  <input type="file" class="form-control" id="Image" name="Image" multiple /><!--image choose button add-->
                </div>
                  
                <div class="mb-3">
                    <label for="Description" class="form-label text-white">Description</label>
                    <textarea class="form-control text-white" id="Description" name="Description"></textarea>
                </div>
                <div class="mb-3">
                    <label for="Quantity" class="form-label text-white">Quantity</label>
                    <textarea class="form-control text-white" id="Description" name="Quantity"></textarea>
                </div>
                <div class="mb-3">
                    <label for="Price" class="form-label text-white">Price</label>
                    <textarea class="form-control text-white" id="Price" name="Price"></textarea>
                </div>
                <button type="submit" class="btn btn-success">Add Prodects</button>
            </form>
